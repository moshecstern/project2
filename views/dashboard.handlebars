<!-- MY PROFILE -->
<div class="containter-fluid">
    <div class="row">
        <div class="col-sm-4 text-center">
            <div class="card">
                <div class="card-header">
            <h3 class="text-black-20 mx-auto mt-2 mb-0">User Profile</h3>
            </div>
            <p>Welcome,</p>
            <p>Liberty Beans Member</p>

            <a href="/logout">
                <i class="glyphicon glyphicon-user"></i>
                Log Out </a>
        </div>
        </div>
        <div class="col-sm-8">
            <div class="card">
                <div class="card-header">
                    <h3 class="text-black-20 mx-auto mt-2 mb-0">Likes</h3>
                </div>
                <div class="card-body">
                    {{#each relations.relation}}
                    <div class="likedlist">

                        <p>{{name}}</p>
                    </div>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
</div>
<hr>
<br>

<div class="container-fluid">
    <div class="row">
        {{#each coffees}}
        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
            <div class="card h-250">
                <img style="height: 200px;" class="card-img-top" src="{{this.img}}" alt="Coffee Image">
                <div class="card-body">
                    <h5 class="card-title">{{this.name}}</h5>
                    <p class="card-text">{{this.company}}</p>
                    <p class="card-text">Type: {{this.flavor}}</p>
                    <p class="card-text">Caffinated: {{this.caf}}</p>

                    <center>
                        <button type="button" class="btn btn-success like2" data-id="{{id}}" value="true"
                            data-newrating="{{liked}}">Like</button>
                    </center>
                </div>
            </div>
        </div>
        {{/each}}

    </div>
</div>

